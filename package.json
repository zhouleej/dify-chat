{
	"name": "dify-chat-root",
	"private": true,
	"version": "0.4.0",
	"scripts": {
		"build": "pnpm -r build",
		"build:pkgs": "pnpm --filter @dify-chat/* build",
		"dev": "./dev-start.sh",
		"dev:docs": "pnpm --filter dify-chat-docs dev",
		"dev:react": "pnpm --filter dify-chat-app-react dev",
		"dev:platform": "pnpm --filter @dify-chat/platform dev",
		"create-admin": "pnpm --filter dify-chat-platform create-admin",
		"test": "pnpm -r test",
		"lint": "pnpm -r lint",
		"format": "pnpm -r format",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release": "pnpm build && changeset publish",
		"docker:build": "./docker-deploy.sh build",
		"docker:start": "./docker-deploy.sh start",
		"docker:stop": "./docker-deploy.sh stop",
		"docker:restart": "./docker-deploy.sh restart",
		"docker:logs": "./docker-deploy.sh logs",
		"docker:cleanup": "./docker-deploy.sh cleanup",
		"docker:deploy": "./docker-deploy.sh deploy"
	},
	"devDependencies": {
		"@changesets/cli": "catalog:",
		"@eslint/compat": "catalog:",
		"@eslint/js": "catalog:",
		"@trivago/prettier-plugin-sort-imports": "catalog:",
		"eslint": "catalog:",
		"eslint-plugin-react": "catalog:",
		"eslint-plugin-react-hooks": "catalog:",
		"globals": "catalog:",
		"husky": "catalog:",
		"lint-staged": "catalog:",
		"prettier": "catalog:",
		"prettier-plugin-tailwindcss": "catalog:",
		"typescript": "catalog:",
		"typescript-eslint": "catalog:"
	},
	"packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
